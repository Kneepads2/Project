@{
    Layout = null;
}
<!DOCTYPE html>
<html>
    <head>
        <style>
            body {
                background-color: rgb(25,25,25);
                font-family: 'Gill Sans', 'Gill Sans MT', Calibri, 'Trebuchet MS', sans-serif            
            }

            .class {
                font-size: 50px;
                color: whitesmoke;
                font-weight: bolder;
            }

            .goodbye {
                font-weight: bold;
                color: whitesmoke;
            }

            .goodbye h2 {
                font-size: 40px;
            }

            .goodbye h3 {
                font-size: 30px;
            }

            a {
                font-size: 30px;
                text-decoration: none;
            }
        </style>
    </head>
    <body>
        <div class="name">
            <h1>Super Body Fitness</h1>
        </div>
        <div class="image">
            <img src="https://upload.wikimedia.org/wikipedia/commons/thumb/3/3b/Eo_circle_green_checkmark.svg/1200px-Eo_circle_green_checkmark.svg.png" />
        </div>
        <div class="goodbye">
            <h2>You have been logged out</h2>
            <h3>Thank you</h3>
        </div>
        <div>
            <a href="/">Return to frontpage</a>
        </div>
    </body>
</html>